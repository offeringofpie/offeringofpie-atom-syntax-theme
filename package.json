{
  "name": "pie-syntax-theme",
  "theme": "syntax",
  "version": "1.0.12",
  "description": "My own personal atom theme, with a 80's revivalism feel",
  "keywords": [
    "atom",
    "syntax",
    "theme",
    "80s",
    "dark",
    "vibrant"
  ],
  "repository": "https://github.com/offeringofpie/pie-syntax-theme",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "main": "lib/main",
  "configSchema": {
    "choice": {
      "title": "Color Theme",
      "description": "What is your flavour",
      "type": "string",
      "default": "Dark",
      "enum": [
        "Dark",
        "Light"
      ]
    }
  }
}
